{
    "name": "mariojgt/gateway",
    "description": "A Laravel Package Gateway, a quick start for new laravel Packages",
    "keywords": [
        "laravel"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "mario",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require":{
        "php": "^7.3|^8.0",
        "stripe/stripe-php": "^7.97.0",
        "gocardless/gocardless-pro": "^4.12",
        "braintree/braintree_php": "^6.4.1"
    },
    "autoload": {
        "psr-4": {
            "Mariojgt\\Gateway\\": "src"
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "Mariojgt\\Gateway\\GatewayProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

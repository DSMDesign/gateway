{
    "name": "mariojgt/gateway",
    "description": "Laravel 7 gateway project, gateway suporte for now stipe.",
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "mario jose goes tarosso",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require":{
        "php": ">=7.2.5",
        "stripe/stripe-php": "^7.3",
        "laravelcollective/html": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "mariojgt\\gateway\\": "src"
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "mariojgt\\gateway\\gatewayServiceProvider"
            ],
            "aliases": {
                "Logit":"mariojgt\\gateway\\Models\\Log"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
